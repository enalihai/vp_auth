<% if Current.user %>
    <h1> Viewing Party Lite</h1><br>
    Logged in as: <%= Current.user.email %><br>
    <%= link_to "Log Out", logout_path, method: :delete %>
<% else %>
    <h1> Viewing Party Lite </h1>
    <%= link_to "Log In", login_path %>
    <%= link_to "Register as a User", register_path %>
<% end %>
